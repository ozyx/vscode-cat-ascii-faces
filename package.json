{
    "name": "vscode-cat-ascii-faces",
    "displayName": "Ascii Cat Faces for VSCode",
    "description": "A mostly useless extension to add happy ascii cat faces into your code!",
    "version": "0.0.1",
    "publisher": "jessemazzella",
    "engines": {
        "vscode": "^1.28.0"
    },
    "keywords": [
        "cat",
        "ascii",
        "cats",
        "fun",
        "art"
    ],
    "license": "MIT",
    "markdown": "github",
    "repository": "https://github.com/ozyx/vscode-cat-ascii-faces",
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:catfaces.insertCatFace",
        "onCommand:catfaces.insertBigCatFace",
        "onCommand:catfaces.insertCatCommentBlock"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "catfaces.insertCatFace",
                "title": "Insert Cat Face"
            },
            {
                "command": "catfaces.insertBigCatFace",
                "title": "Insert Big Cat Face"
            },
            {
                "command": "catfaces.insertCatCommentBlock",
                "title": "Insert Cat Comment Block"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/chai": "^4.1.6",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "chai": "^4.2.0",
        "event-stream": "^3.3.4",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    }
}
